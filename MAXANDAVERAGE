public static int max(int[]m data) {
	int max = data[0];
	for (int i = 1; i < data.length; i++) {
		if (data[i] > max) {
			Max = data[i];
		}
}
return max;

}
-----
 public static double average(int[] a) {
	double mean = 0.0;
	for (int i = 0; i < a.length; i++) {
mean += a[i];

}
return mean / a.length;

}
